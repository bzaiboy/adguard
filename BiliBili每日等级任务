hostname=app.bilibili.com，m.bilibili.com

^https?:\/\/app\.bilibili\.com\/x\/resource\/domain\? url script-response-body https://raw.githubusercontent.com/ClydeTime/BiliBili/main/js/BiliBiliDailyBonus.js
^https?:\/\/m.bilibili.com/$ url script-response-body https://raw.githubusercontent.com/ClydeTime/BiliBili/main/js/BiliBiliDailyBonus.js
